{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "ecmaFeatures": {
      "ts": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-debugger": "off",
    "no-console": "off",
    "no-var": 1,
    "camelcase": 1,
    "no-nested-ternary": 1,
    "no-template-curly-in-string": 1,
    "no-self-compare": 1,
    "semi": [
      1
    ],
    "max-len": [
      "error",
      {
        "ignoreUrls": true,
        "ignoreComments": true,
        "ignoreRegExpLiterals": true,
        "code": 80
      }
    ]
  },
  "extends": [
    "eslint:recommended"
  ],
  "globals": {},
  "env": {
    "es2021": true,
    "browser": true,
    "node": true,
    "commonjs": true,
    "jquery": true
  }
}
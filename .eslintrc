{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "ecmaFeatures": {
      "ts": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-debugger": "off",
    "no-console": "off",
    "no-var": 1,
    "camelcase": 1,
    "no-nested-ternary": "error",
    "no-template-curly-in-string": 1,
    "no-self-compare": 1,
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "import/no-extraneous-dependencies": "off",
    "no-underscore-dangle": "off",
    "no-new": "off",
    "no-loss-of-precision": "off",
    "nonblock-statement-body-position": "error",
    "semi": [
      1
    ],
    "max-len": [
      "error",
      {
        "ignoreUrls": true,
        "ignoreComments": true,
        "ignoreRegExpLiterals": true,
        "code": 100
      }
    ]
  },
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "es2021": true,
    "browser": true,
    "node": true,
    "commonjs": true,
    "jquery": true
  }
}